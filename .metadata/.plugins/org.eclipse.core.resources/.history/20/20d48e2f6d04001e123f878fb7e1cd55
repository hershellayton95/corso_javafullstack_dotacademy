package lezione3.forme;

public interface IShape {
	int x;
	int y;

	 boolean isVisible;

	public int getX();

	public void setX();

	public int getY();

	public void setY();

	public boolean isVisible();

	public void setVisible();

	public abstract void show();

	public abstract void hide();
	
	public void move(int x, int y) {
		System.out.println(this);
		this.hide();
		setX(x);
		setY(y);
		this.show();
		System.out.println(this);
	}
	
}
